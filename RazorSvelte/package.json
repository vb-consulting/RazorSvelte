{
    "type": "commonjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "postinstall": "node ./Scripts/postinstall.js",
        "dotnet-watch": "dotnet watch",
        "dotnet-run": "dotnet run",
        "dotnet-clean": "dotnet clean",
        "dotnet-build": "dotnet build",
        "dotnet-audit": "dotnet list package --vulnerable --include-transitive",
        "build-urls": "dotnet run -- build-urls",
        "build-models": "dotnet run -- build-models",
        "fe-scss-build": "sass ./App/scss/style.scss ./wwwroot/style.css --no-source-map --style compressed",
        "fe-scss-watch": "sass ./App/scss/style.scss ./wwwroot/style.css -w --style compressed",
        "fe-build": "node ./Scripts/build.js",
        "fe-watch": "node ./Scripts/build.js -w",
        "fe-build-all": "npm run format && npm run lint && node ./Scripts/build-all.js",
        "fe-watch-all": "node ./Scripts/watch-all.js",
        "npm-upgrade": "ncu --upgrade && npm install && npm audit fix",
        "npm-audit-fix": "npm audit fix",
        "code": "code .",
        "new-page": "node ./Scripts/new-page.js && dotnet run -- build-urls",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@material-design-icons/font": "^0.14.11",
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.2",
        "@tsconfig/svelte": "^5.0.0",
        "@types/bootstrap": "^5.2.6",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "bootstrap": "5.3.1",
        "bootstrap-icons": "^1.10.5",
        "chart.js": "^4.3.3",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-svelte": "^2.32.4",
        "mdb-ui-kit": "^6.4.1",
        "npm-check-updates": "^16.12.2",
        "prettier": "^3.0.2",
        "prettier-plugin-svelte": "^3.0.3",
        "rollup": "^3.28.0",
        "rollup-plugin-css-only": "^4.3.0",
        "rollup-plugin-svelte": "^7.1.6",
        "sass": "^1.66.0",
        "svelte": "^4.2.0",
        "svelte-check": "^3.5.0",
        "svelte-preprocess": "^5.0.4",
        "svelte-spa-router": "^3.3.0",
        "tslib": "^2.6.1",
        "typescript": "^5.1.6"
    }
}
