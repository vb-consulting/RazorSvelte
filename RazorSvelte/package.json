{
    "type": "commonjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "postinstall": "node ./Scripts/postinstall.js",
        "dotnet-watch": "dotnet watch",
        "dotnet-run": "dotnet run",
        "dotnet-clean": "dotnet clean",
        "dotnet-build": "dotnet build",
        "dotnet-audit": "dotnet list package --vulnerable --include-transitive",
        "build-urls": "dotnet run -- build-urls",
        "build-models": "dotnet run -- build-models",
        "fe-scss-build": "sass ./App/scss/style.scss ./wwwroot/style.css --no-source-map --style compressed",
        "fe-scss-watch": "sass ./App/scss/style.scss ./wwwroot/style.css -w --style compressed",
        "fe-build": "node ./Scripts/build.js",
        "fe-watch": "node ./Scripts/build.js -w",
        "fe-build-all": "npm run format && node ./Scripts/build-all.js",
        "fe-watch-all": "node ./Scripts/watch-all.js",
        "npm-upgrade": "ncu --upgrade && npm install && npm audit fix",
        "npm-audit-fix": "npm audit fix",
        "code": "code .",
        "new-page": "node ./Scripts/new-page.js && dotnet run -- build-urls",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@material-design-icons/font": "^0.14.13",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@tsconfig/svelte": "^5.0.4",
        "@types/bootstrap": "^5.2.10",
        "bootstrap": "5.3.3",
        "bootstrap-icons": "^1.11.3",
        "chart.js": "^4.4.2",
        "mdb-ui-kit": "^7.2.0",
        "npm-check-updates": "^16.14.20",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.3",
        "rollup": "^4.16.4",
        "rollup-plugin-css-only": "^4.5.2",
        "rollup-plugin-svelte": "^7.2.0",
        "sass": "^1.75.0",
        "svelte": "^4.2.15",
        "svelte-check": "^3.7.0",
        "svelte-preprocess": "^5.1.4",
        "svelte-spa-router": "^4.0.1",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5"
    }
}
