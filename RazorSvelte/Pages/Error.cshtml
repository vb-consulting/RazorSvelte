@page
@attribute [RazorCompiledItemMetadata("RouteTemplate", Urls.ErrorUrl)]
@model ErrorModel
@{
    ViewData["Title"] = "Error";
}

@section HeadSection {
    <link href="~/build/error.css" asp-append-version="true" rel="stylesheet" />
    <script defer src="~/build/error.js" asp-append-version="true"></script>
}

<input id="error-key" type="hidden" value='@Consts.ErrorKey' />
